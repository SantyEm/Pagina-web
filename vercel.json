{
  "version": 2,
  "builds": [
    {
      "src": "HomeProcesos.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/static/css/stiloMenu.css",
      "destination": "/static/css/stiloMenu.css"
    },
    {
      "source": "/static/css/stylesHome.css",
      "destination": "/static/css/stylesHome.css"
    },
    {
      "source": "/static/js/menu.js",
      "destination": "/static/js/menu.js"
    }
    ,
    {
      "source": "/opciones_usuario",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/acerca-de",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/volver_inicio",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/backup",
      "destination": "HomeProcesos.py"
    }
  ]
}