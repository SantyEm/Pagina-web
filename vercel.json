{
  "version": 2,
  "builds": [
    {
      "src": "HomeProcesos.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/static/css/(.*)",
      "destination": "/static/css/$1"
    },
    {
      "source": "/opciones_usuario",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/acerca-de",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/volver_inicio",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/backup",
      "destination": "HomeProcesos.py"
    }
  ]
}