{
  "version": 2,
  "builds": [
    {
      "src": "HomeProcesos.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/static/css/(.*)",
      "destination": "/static/css/$1"
    }
  ]
}