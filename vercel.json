{
  "version": 2,
  "builds": [
    {
      "src": "HomeProcesos.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "HomeProcesos.py"
    },
    {
      "src": "/static/css/(.*)",
      "dest": "static/css"
    },
    {
      "src": "/static/img/(.*)",
      "dest": "static/img"
    },
    {
      "src": "/static/js/(.*)",
      "dest": "static/js"
    }
  ]
}