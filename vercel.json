{
  "version": 2,
  "builds": [
    {
      "src": "HomeProcesos.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "HomeProcesos.py"
    },
    {
      "source": "/static/css/(.*)",
      "destination": "/static/css/$1"
    }
  ],
  "routes": [
    {
      "src": "/opciones_usuario",
      "dest": "/opciones_usuario"
    },
    {
      "src": "/acerca-de",
      "dest": "/acerca-de"
    },
    {
      "src": "/volver_inicio",
      "dest": "/volver_inicio"
    },
    {
      "src": "/backup",
      "dest": "/backup"
    }
  ]
}